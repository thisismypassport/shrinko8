function print(value)
    -- TODO: tostr(value, true) doesn't do this right now..
    if type(value) == "number" then
        if math.type(value) == "integer" then
            value = string.format("0x%x", value)
        else
            value = string.format("%a", value)
        end
    end
    printh(tostr(value))
end
